.my-box {
  position: fixed;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 99999999;
  display: flex;
  justify-content: center;
  align-items: center;
}

.fade {
  animation-name: fade;
  animation-duration: .5s;
  animation-timing-function: linear
}

.my-box .main {
  background-color: white;
  padding: 50rpx 50rpx;
  padding-top: 0;
  width: 80%;
  border-radius: 15rpx;
}

.title {
  font-size: 30rpx;
  font-weight: bold;
  padding: 20rpx 0;
}

