.main {
  margin: 40rpx 20rpx;
  margin-bottom: 10%;
  background-color: white;
}

.main .my-top-bar {
  padding: 10rpx 50rpx;
}

image {
  background-color: transparent !important;
}

.footer {
  margin-bottom: 5rpx;
}

.feedback-box {
  margin: 0 30rpx;
  margin-bottom: 30rpx;
}

.no-record {
  margin: 35% 100rpx;
  text-align: center;
  font-size: larger;
}

.no-record>view {
  margin-bottom: 50rpx;
  font-size: 150rpx;
}


.my-feedback {
  margin: 30rpx 0;
  padding: 30rpx 30rpx;
  border-radius: 10rpx;
}


.my-feedback .my-content {
  padding: 0 5rpx;
  flex-direction: column;
  border-bottom: 2rpx solid #eee;
}

.my-feedback .my-content>view {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 20rpx 20rpx;
}

.my-feedback .my-content .info {
  display: flex;
  flex-grow: 1;
  flex-direction: column;
  padding-left: 30rpx;
  justify-content: center;
}

.my-feedback .my-content .info>view {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.my-feedback .my-bottom {
  display: flex;
  flex-direction: column;
  margin: 15rpx;
}

.my-feedback .my-bottom>view {
  margin: 15rpx 0;
}

.my-feedback .my-bottom .info {
  display: flex;
  justify-content: space-between;
}

.my-feedback .my-bottom .other-price {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  font-weight: bold;
}

.my-bar {
  position: fixed;
  bottom: 80rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.my-bar .page-bar {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  padding: 15rpx 50rpx;
  background-color: white;
  border: 5rpx #eee solid;
  border-radius: 5000rpx;
  width: 80%;
}

my-box .feedback-group {
  padding: 10rpx 15rpx;
  color: #333;
}

my-box .feedback-group > .content{
  padding: 10rpx 30rpx;
  color: #666;
}

my-box .my-btn-group {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  margin: 30rpx;
}